{
  "name": "elmsinki-site",
  "version": "1.0.0",
  "license": "BSD-3-clause",
  "devDependencies": {
    "chokidar-cli": "^1.2.2",
    "concurrently": "^4.1.0",
    "serve": "^10.1.2",
    "static-html-from-elm": "^1.0.3"
  },
  "scripts": {
    "dev": "concurrently \"npm:watch\" \"npm:serve\"",
    "build": "npx static-html-from-elm --config elm-static-html.json",
    "watch": "chokidar \"src/*.elm\" --command \"npm run build\" --initial",
    "serve": "serve --listen 8080"
  }
}
